{
  "name": "what-broke-ui",
  "version": "0.0.0-development",
  "description": "browser UI for viewing breaking changes to packages in your project",
  "bin": {
    "what-broke-ui": "./assets/server/index.js"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "sideEffects": false,
  "scripts": {
    "lint": "eslint src test --cache",
    "lint:fix": "eslint --fix src test --cache",
    "lint:watch": "esw --watch src test --cache",
    "prettier": "prettier --write *.json *.md *.js src/**/*.js test/**/*.js",
    "prettier:check": "prettier --list-different *.json *.md *.js src/**/*.js test/**/*.js",
    "flow": "flow",
    "flow:coverage": "for file in src/**.js test/**.js; do echo $file; flow coverage $file; done",
    "flow:watch": "flow-watch -e js,js.flow,flowconfig --ignore es/ --ignore node_modules/ --watch .flowconfig --watch src/ --watch test/",
    "clean": "rimraf assets",
    "build": "npm run clean && cross-env BABEL_ENV=production babel src --out-dir assets && cross-env NODE_ENV=production webpack",
    "test": "cross-env NODE_ENV=production BABEL_ENV=es5 mocha $npm_package_config_mocha && cross-env NODE_ENV=production BABEL_ENV=coverage nyc --reporter=lcov --reporter=text mocha $npm_package_config_mocha",
    "test:watch": "cross-env NODE_ENV=production BABEL_ENV=test mocha --watch $npm_package_config_mocha",
    "test:debug": "cross-env NODE_ENV=production BABEL_ENV=test mocha --inspect-brk $npm_package_config_mocha",
    "codecov": "nyc report --reporter=text-lcov > coverage.lcov; codecov",
    "prepublishOnly": "npm run clean && npm run prettier:check && npm run lint && flow && npm test && npm run build",
    "open:coverage": "open coverage/lcov-report/index.html",
    "semantic-release": "semantic-release",
    "start": "defaultenv env/dev.js -- cross-env NODE_ENV=development node scripts/dev.js",
    "built": "node assets/server/index.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run lint && flow",
      "commit-msg": "commitlint -e $GIT_PARAMS",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@jedwards1211/commitlint-config"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "config": {
    "mocha": "-r @babel/register -r jsdom-global/register test/configure.js test/**.js",
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "require": [
      "@babel/register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jedwards1211/what-broke-ui.git"
  },
  "keywords": [
    ""
  ],
  "author": "Andy Edwards",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jedwards1211/what-broke-ui/issues"
  },
  "homepage": "https://github.com/jedwards1211/what-broke-ui#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@commitlint/cli": "^6.0.2",
    "@commitlint/config-conventional": "^6.0.2",
    "@jedwards1211/commitlint-config": "^1.0.0",
    "@jedwards1211/eslint-config": "^2.0.0",
    "@jedwards1211/eslint-config-flow": "^1.0.2",
    "@jedwards1211/eslint-config-react": "^4.0.0",
    "assets-webpack-plugin": "^3.9.10",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-flow-react-proptypes": "^24.1.2",
    "babel-plugin-istanbul": "^5.1.0",
    "chai": "^4.2.0",
    "codecov": "^3.1.0",
    "copy": "^0.3.2",
    "cross-env": "^5.2.0",
    "defaultenv": "^3.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-watch": "^4.0.2",
    "flow-bin": "^0.92.0",
    "flow-copy-source": "^2.0.2",
    "flow-watch": "^1.1.4",
    "http-proxy": "^1.17.0",
    "husky": "^1.1.4",
    "istanbul": "^0.4.5",
    "jsdom": "^11.5.1",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^8.0.4",
    "mocha": "^6.0.0",
    "nyc": "^13.1.0",
    "prettier": "^1.15.2",
    "prettier-eslint": "^8.8.2",
    "rimraf": "^2.6.0",
    "semantic-release": "^15.13.3",
    "smart-restart": "^2.1.0",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-middleware": "^3.6.2",
    "webpack-dev-server": "^3.3.1",
    "webpack-hot-middleware": "^2.24.4"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.5",
    "@jcoreio/require-env": "^1.0.11",
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-link-http": "^1.5.14",
    "apollo-link-schema": "^1.2.2",
    "apollo-server-express": "^2.4.8",
    "autoprefixer": "^9.5.1",
    "body-parser": "^1.19.0",
    "classnames": "^2.2.6",
    "express": "^4.16.4",
    "fs-extra": "^7.0.1",
    "graphql": "^14.2.1",
    "graphql-iso-date": "^3.6.1",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.4",
    "graphql-type-json": "^0.3.0",
    "jss": "^9.8.7",
    "jss-preset-default": "^4.5.0",
    "lodash": "^4.17.11",
    "material-ui-render-props-styles": "^4.0.4",
    "open": "^6.2.0",
    "portscanner": "^2.2.0",
    "postcss": "^7.0.14",
    "prop-types": "^15.0.0",
    "react": "^16.8.0",
    "react-apollo": "^2.5.5",
    "react-dom": "^16.8.0",
    "react-jss": "^8.6.1",
    "react-markdown": "^4.0.8",
    "react-router-dom": "^5.0.0",
    "require-glob": "^3.2.0",
    "what-broke": "^1.3.0"
  },
  "renovate": {
    "extends": [
      ":separateMajorReleases",
      ":combinePatchMinorReleases",
      ":ignoreUnstable",
      ":prImmediately",
      ":renovatePrefix",
      ":updateNotScheduled",
      ":preserveSemverRanges",
      ":semanticPrefixFixDepsChoreOthers",
      ":automergeDisabled",
      "group:monorepos"
    ],
    "automerge": true,
    "major": {
      "automerge": false
    }
  }
}
